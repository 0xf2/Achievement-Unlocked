#! /bin/bash
#
# achievement: publish a humorous "Achievement Unlocked" popup window 
# Released under the MIT license.
# 
# (c) 2012 by Federico Lucifredi
# 
# HISTORY:
# 1.0 Initial release
# 1.1 Added Growl support
# 1.2 Added embedded binary icon
#
# TODO:
#

UNAME=`uname`

function untar_payload()
{
	MATCH=$(grep --text --line-number '^PAYLOAD:$' $0 | cut -d ':' -f 1)
	PAYLOAD_START=$((MATCH + 1))

	if [ "$UNAME" == "Darwin" ]
	then
	  tail -n +$PAYLOAD_START $0 | uudecode -p | tar -P -zxvf - &> /dev/null
	elif [ "$UNAME" == "Linux" ]
	then
	  tail -n +$PAYLOAD_START $0 | uudecode | tar -P -zxvf - &> /dev/null
	else
	  echo "Unknown host platform. Get a real OS!"
	fi
}

if [ ! -n "$1" ]
then
  echo "Usage: `basename $0` message"
  exit 1
fi

if [ ! -e "/tmp/achievement_unlocked.png" ]
then 
  untar_payload
fi

if [ -x /usr/bin/notify-send ]
then
  notify-send -i /tmp/achievement_unlocked.png "Achievement Unlocked" "$*"
elif [ -x /usr/local/bin/growlnotify ]
then
  growlnotify --image /tmp/achievement_unlocked.png  -m "$*" Achievement Unlocked
else
  echo "notification infrastructure not installed"
  exit 1
fi

exit 0

# Add Payload section with the following:
# echo "PAYLOAD:" >> achievement; cat icon.tar.gz | uuencode - >> achievement 
# use -P to force absolute path at archive creation time
# 
PAYLOAD:
